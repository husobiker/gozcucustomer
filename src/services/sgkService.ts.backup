  ): Promise<SgkResponse> {
    if (!this.credentials) {
      throw new Error("SGK kimlik bilgileri bulunamadÄ±");
    }

    try {
      const urls = this.getUrls();
      const kullaniciBilgileri = this.getKullaniciBilgileri();

      const response = await fetch(
        `${urls.iseGiris}/tckimlikNoTarihileiseGirisSorgula`,
        {
          method: "POST",
          headers: {
            "Content-Type": "application/json",
          },
          body: JSON.stringify({
            kullaniciBilgileri,
            tcKimlikNo: tckimlikNo,
            iseGirisTarihi: this.formatTarih(new Date(iseGirisTarihi)),
          }),
        }
      );

      if (!response.ok) {
        throw new Error(`HTTP ${response.status}: ${response.statusText}`);
      }

      const data = await response.json();
      return data;
    } catch (error) {
      console.error("SGK TC+Tarih Ä°ÅŸe GiriÅŸ Sorgulama HatasÄ±:", error);
      throw new Error(`SGK servisine baÄŸlanÄ±lamadÄ±: ${error}`);
    }
  }

