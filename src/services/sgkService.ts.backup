  // SGK Eksik GÃ¼n Nedeni Validasyonu
  validateEksikGunNedeni(
    eksikGunNedeni: string,
    baslangicTarihi: string,
    bitisTarihi: string,
    calismaGunu: number
  ): {
    isValid: boolean;
    uyarilar: string[];
    oneriler: string[];
    sgkBildirimi: boolean;
    belgeGerekli: boolean;
  } {
    const uyarilar: string[] = [];
    const oneriler: string[] = [];
    let sgkBildirimi = true;
    let belgeGerekli = true;

    try {
      const eksikGunNedenleri = this.getEksikGunNedenleri();
      const secilenNeden = eksikGunNedenleri.find(
        (neden) => neden.kod === eksikGunNedeni
      );

      if (!secilenNeden) {
        uyarilar.push("GeÃ§ersiz eksik gÃ¼n nedeni kodu");
        oneriler.push("GeÃ§erli bir eksik gÃ¼n nedeni seÃ§in");
        return {
          isValid: false,
          uyarilar,
          oneriler,
          sgkBildirimi: false,
          belgeGerekli: false,
        };
      }

      // Tarih kontrolleri
      const baslangic = new Date(baslangicTarihi);
      const bitis = new Date(bitisTarihi);
      const bugun = new Date();

      if (baslangic > bitis) {
        uyarilar.push("BaÅŸlangÄ±Ã§ tarihi bitiÅŸ tarihinden sonra olamaz");
        oneriler.push("Tarihleri kontrol edin");
      }

      if (bitis > bugun) {
        uyarilar.push("BitiÅŸ tarihi gelecekte olamaz");
        oneriler.push("BitiÅŸ tarihini kontrol edin");
      }

      // Ã‡alÄ±ÅŸma gÃ¼nÃ¼ kontrolleri
      if (calismaGunu <= 0) {
        uyarilar.push("Ã‡alÄ±ÅŸma gÃ¼nÃ¼ 0 veya negatif olamaz");
        oneriler.push("Ã‡alÄ±ÅŸma gÃ¼nÃ¼nÃ¼ kontrol edin");
      }

      // Kategori bazlÄ± kontroller
      switch (secilenNeden.kategori) {
        case "izin":
          if (calismaGunu > 30) {
            uyarilar.push("Ä°zin sÃ¼resi 30 gÃ¼nden fazla olamaz");
            oneriler.push("Ä°zin sÃ¼resini kontrol edin");
          }
          break;
        case "rapor":
          if (calismaGunu > 365) {
            uyarilar.push("Rapor sÃ¼resi 365 gÃ¼nden fazla olamaz");
            oneriler.push("Rapor sÃ¼resini kontrol edin");
          }
          break;
        case "disiplin":
          if (calismaGunu > 90) {
            uyarilar.push("Disiplin cezasÄ± sÃ¼resi 90 gÃ¼nden fazla olamaz");
            oneriler.push("Disiplin cezasÄ± sÃ¼resini kontrol edin");
          }
          break;
        case "diger":
          if (calismaGunu > 180) {
            uyarilar.push("DiÄŸer nedenler iÃ§in sÃ¼re 180 gÃ¼nden fazla olamaz");
            oneriler.push("SÃ¼reyi kontrol edin");
          }
          break;
      }

      // Ã–zel durumlar
      if (secilenNeden.kod === "01" && calismaGunu < 14) {
        uyarilar.push("YÄ±llÄ±k izin en az 14 gÃ¼n olmalÄ±dÄ±r");
        oneriler.push("YÄ±llÄ±k izin sÃ¼resini kontrol edin");
      }

      if (secilenNeden.kod === "03" && calismaGunu < 105) {
        uyarilar.push("DoÄŸum izni en az 105 gÃ¼n olmalÄ±dÄ±r");
        oneriler.push("DoÄŸum izni sÃ¼resini kontrol edin");
      }

      return {
        isValid: uyarilar.length === 0,
        uyarilar,
        oneriler,
        sgkBildirimi,
        belgeGerekli,
      };
    } catch (error) {
      return {
        isValid: false,
        uyarilar: ["Eksik gÃ¼n nedeni validasyon hatasÄ±"],
        oneriler: ["Verileri kontrol edin"],
        sgkBildirimi: false,
        belgeGerekli: false,
      };
    }
  }

