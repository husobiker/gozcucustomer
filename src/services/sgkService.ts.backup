  // SGK DÃ¶nem BaÅŸlangÄ±Ã§/BitiÅŸ Tarihleri sistemi
  calculateDonemTarihleri(
    isyeriTuru: "ozel" | "resmi",
    yil: number,
    ay: number
  ): {
    donemBaslangic: string;
    donemBitis: string;
    donemKodu: string;
    aciklama: string;
  } {
    try {
      let donemBaslangic: Date;
      let donemBitis: Date;

      if (isyeriTuru === "ozel") {
        // Ã–zel iÅŸyerleri: AyÄ±n 1'i baÅŸlangÄ±Ã§
        donemBaslangic = new Date(yil, ay - 1, 1);
        donemBitis = new Date(yil, ay, 0); // AyÄ±n son gÃ¼nÃ¼
      } else {
        // Resmi iÅŸyerleri: AyÄ±n 15'i baÅŸlangÄ±Ã§
        donemBaslangic = new Date(yil, ay - 1, 15);
        // Bir sonraki ayÄ±n 14'Ã¼ bitiÅŸ
        donemBitis = new Date(yil, ay, 14);
      }

      const donemKodu = `${yil}${ay.toString().padStart(2, "0")}`;
      const aciklama =
        isyeriTuru === "ozel"
          ? `Ã–zel iÅŸyeri dÃ¶nemi: ${ay}/${yil}`
          : `Resmi iÅŸyeri dÃ¶nemi: ${ay}/${yil} (15'inden baÅŸlar)`;

      return {
        donemBaslangic: donemBaslangic.toISOString().split("T")[0],
        donemBitis: donemBitis.toISOString().split("T")[0],
        donemKodu,
        aciklama,
      };
    } catch (error) {
      throw new Error("DÃ¶nem tarihi hesaplama hatasÄ±");
    }
  }

  // SGK DÃ¶nem Validasyonu
  validateDonemTarihleri(
    isyeriTuru: "ozel" | "resmi",
    girisTarihi: string,
    cikisTarihi?: string
  ): {
    isValid: boolean;
    donemBilgileri: any[];
    uyarilar: string[];
    oneriler: string[];
  } {
    const uyarilar: string[] = [];
    const oneriler: string[] = [];
    const donemBilgileri: any[] = [];

    try {
      const giris = new Date(girisTarihi);
      const cikis = cikisTarihi ? new Date(cikisTarihi) : new Date();

      // GiriÅŸ ve Ã§Ä±kÄ±ÅŸ tarihleri arasÄ±ndaki tÃ¼m dÃ¶nemleri hesapla
      const currentDate = new Date(giris);

      while (currentDate <= cikis) {
        const yil = currentDate.getFullYear();
        const ay = currentDate.getMonth() + 1;

        const donemBilgisi = this.calculateDonemTarihleri(isyeriTuru, yil, ay);

        // Bu dÃ¶nemde Ã§alÄ±ÅŸma var mÄ± kontrol et
        const donemBaslangic = new Date(donemBilgisi.donemBaslangic);
        const donemBitis = new Date(donemBilgisi.donemBitis);

        const donemGirisTarihi = new Date(
          Math.max(donemBaslangic.getTime(), giris.getTime())
        );
        const donemCikisTarihi = new Date(
          Math.min(donemBitis.getTime(), cikis.getTime())
        );

        if (donemGirisTarihi <= donemCikisTarihi) {
          donemBilgileri.push({
            ...donemBilgisi,
            donemGirisTarihi: donemGirisTarihi.toISOString().split("T")[0],
            donemCikisTarihi: donemCikisTarihi.toISOString().split("T")[0],
            calismaGunu: this.calculateCalismaGunu(
              donemGirisTarihi,
              donemCikisTarihi
            ),
          });
        }

        // Bir sonraki aya geÃ§
        currentDate.setMonth(currentDate.getMonth() + 1);
        currentDate.setDate(1);
      }

      // DÃ¶nem kontrolleri
      if (donemBilgileri.length === 0) {
        uyarilar.push("HiÃ§bir dÃ¶nemde Ã§alÄ±ÅŸma bulunamadÄ±");
        oneriler.push("GiriÅŸ ve Ã§Ä±kÄ±ÅŸ tarihlerini kontrol edin");
      } else if (donemBilgileri.length > 12) {
        uyarilar.push("Ã‡ok fazla dÃ¶nem kapsanÄ±yor (12+ ay)");
        oneriler.push("Ã‡Ä±kÄ±ÅŸ tarihini kontrol edin");
      }

      // Ã–zel iÅŸyeri kontrolleri
      if (isyeriTuru === "ozel") {
        donemBilgileri.forEach((donem, index) => {
          const donemBaslangic = new Date(donem.donemBaslangic);
          if (donemBaslangic.getDate() !== 1) {
            uyarilar.push(
              `${donem.donemKodu} dÃ¶nemi Ã¶zel iÅŸyeri iÃ§in ayÄ±n 1'inde baÅŸlamalÄ±`
            );
          }
        });
      }

      // Resmi iÅŸyeri kontrolleri
      if (isyeriTuru === "resmi") {
        donemBilgileri.forEach((donem, index) => {
          const donemBaslangic = new Date(donem.donemBaslangic);
          if (donemBaslangic.getDate() !== 15) {
            uyarilar.push(
              `${donem.donemKodu} dÃ¶nemi resmi iÅŸyeri iÃ§in ayÄ±n 15'inde baÅŸlamalÄ±`
            );
          }
        });
      }

      return {
        isValid: uyarilar.length === 0,
        donemBilgileri,
        uyarilar,
        oneriler,
      };
    } catch (error) {
      return {
        isValid: false,
        donemBilgileri: [],
        uyarilar: ["DÃ¶nem hesaplama hatasÄ±"],
        oneriler: ["Tarih formatÄ±nÄ± kontrol edin"],
      };
    }
  }

  // Ã‡alÄ±ÅŸma gÃ¼nÃ¼ hesaplama yardÄ±mcÄ± fonksiyonu
  private calculateCalismaGunu(baslangic: Date, bitis: Date): number {
    let calismaGunu = 0;
    const currentDate = new Date(baslangic);

    while (currentDate <= bitis) {
      const dayOfWeek = currentDate.getDay();
      if (dayOfWeek >= 1 && dayOfWeek <= 5) {
        calismaGunu++;
      }
      currentDate.setDate(currentDate.getDate() + 1);
    }

    return calismaGunu;
  }

